# VoiceMaps - Environment Variables Template
# Copy this file to .env and add your real API keys for production deployment

# Minimax API - Voice Generation Service
# Get your API key from: https://api.minimax.chat/
REACT_APP_MINIMAX_API_KEY=your_minimax_api_key_here

# Apify API - Google Maps Scraping Service  
# Get your token from: https://console.apify.com/
REACT_APP_APIFY_TOKEN=your_apify_token_here

# Redis Database - Conversation Memory Storage
# Use Redis Cloud, AWS ElastiCache, or local Redis instance
REACT_APP_REDIS_URL=redis://localhost:6379

# Optional: Custom configuration
REACT_APP_SESSION_TIMEOUT=3600
REACT_APP_MAX_SEARCH_RESULTS=5
REACT_APP_DEFAULT_LOCATION=San Francisco, CA

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_ENABLE_CONSOLE_LOGS=true

# Production Deployment
# Set these in your hosting platform (Vercel, Netlify, etc.)
# REACT_APP_MINIMAX_API_KEY=prod_minimax_key
# REACT_APP_APIFY_TOKEN=prod_apify_token  
# REACT_APP_REDIS_URL=prod_redis_url
